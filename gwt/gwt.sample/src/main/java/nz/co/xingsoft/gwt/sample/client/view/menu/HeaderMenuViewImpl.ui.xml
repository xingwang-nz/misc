<!DOCTYPE ui:UiBinder SYSTEM "xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.header {
			background-color:#2A99B8;
			padding-left: 0px;
				padding-bottom: 3px;
			}
		
	</ui:style>

	<g:DockLayoutPanel width="100%" height="100%" ui:field="pnlMainHeader" styleName="{style.header}"
		unit="PX">
		<g:center>
			<g:SimplePanel width="100%" >
				<g:Image ui:field="appLogo" debugId="appLogo" />
			</g:SimplePanel>
		</g:center>
		
	</g:DockLayoutPanel>
</ui:UiBinder>